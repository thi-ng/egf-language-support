{
  "name": "egf-language-support",
  "displayName": "EGF language support",
  "publisher": "thi-ng",
  "description": "VSCode language support for Extensible Graph Format (https://thi.ng/egf)",
  "version": "0.0.1",
  "author": "Karsten Schmidt <k+npm@thi.ng>",
  "repository": {
    "type": "git",
    "url": "https://github.com/thi-ng/egf-language-support.git"
  },
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "egf",
        "aliases": [
          "Extensible Graph Format",
          "egf"
        ],
        "extensions": [
          ".egf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "egf",
        "scopeName": "source.egf",
        "path": "./syntaxes/egf.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "egf",
        "path": "./snippets.json"
      }
    ]
  }
}
